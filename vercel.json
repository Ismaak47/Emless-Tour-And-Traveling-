{
  "routes": [
    {
      "src": "/zanzibar-tours",
      "dest": "/index.html"
    },
    {
      "src": "/tanzania-safaris",
      "dest": "/index.html"
    },
    {
      "src": "/activities",
      "dest": "/index.html"
    },
    {
      "src": "/about",
      "dest": "/index.html"
    },
    {
      "src": "/contact",
      "dest": "/index.html"
    },
    {
      "src": "/tour/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/book/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}